{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": false,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "EXPO_AUTH_POPUP_BEHAVIOR": "popup"
      }
    },
    "preview": {
      "extends": "development",
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "extends": "development",
      "distribution": "store",
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}